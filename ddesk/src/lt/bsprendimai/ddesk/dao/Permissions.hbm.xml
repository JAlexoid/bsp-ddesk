<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="lt.bsprendimai.ddesk.dao.Permission" table="permissions">
        <id column="id" name="id" type="java.lang.Integer">
            <generator class="sequence">
                <param name="sequence">permissions_id_seq</param>
            </generator>           
        </id>
<property column="code" length="200" name="code" not-null="true" type="java.lang.String"/>
<property column="person" length="4" name="person" not-null="true" type="java.lang.Integer"/>
</class>
</hibernate-mapping>
