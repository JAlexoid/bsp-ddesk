<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="lt.bsprendimai.ddesk.dao.PersonInfo" table="person_info"  mutable="false">
        <id column="id" name="id" type="java.lang.Integer">
            <generator class="assigned"/>           
        </id>
        <property column="name" length="250" name="name" type="java.lang.String"/>
        <property column="email" length="100" name="email" type="java.lang.String"/>
        <property column="project" length="200" name="project" type="java.lang.String"/>
        <property column="code" length="200" name="code" type="java.lang.String"/>
        <property column="project_id" name="projectId" type="java.lang.Integer"/>
        <property column="company" length="300" name="company" type="java.lang.String"/>
        <property column="company_id" name="companyId" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>
