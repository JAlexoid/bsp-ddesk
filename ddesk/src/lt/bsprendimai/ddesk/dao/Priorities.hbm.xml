<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="lt.bsprendimai.ddesk.dao.Priority" table="priorities">
        <id column="id" name="id" type="java.lang.Integer">
            <generator class="assigned">
                <!--param name="sequence">priorities_id_seq</param-->
            </generator>
        </id>
        <property column="name" length="100" name="name" not-null="true" type="java.lang.String"/>
        <property column="timing" name="timing" type="java.lang.Double" not-null="true"/>
    </class>
</hibernate-mapping>