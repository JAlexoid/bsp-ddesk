<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="lt.bsprendimai.ddesk.dao.ProjectInfo" table="project_info"  mutable="false">
        <id column="id" name="id" type="java.lang.Integer">
            <generator class="assigned"/>           
        </id>
        <property column="name" length="200" name="name" type="java.lang.String"/>
        <property column="code" length="200" name="code" type="java.lang.String"/>
        <property column="company" length="4" name="company" type="java.lang.Integer"/>
        <property column="person" length="4" name="person" type="java.lang.Integer"/>
        <property column="manager" length="4" name="manager" type="java.lang.Integer"/>
        <property column="description" name="description" type="java.lang.String"/>
        <property column="version" length="10" name="version" type="java.lang.Double"/>
        <property column="company_name" length="300" name="companyName" type="java.lang.String"/>
        <property column="person_name" length="250" name="personName" type="java.lang.String"/>
        <property column="manager_name" length="250" name="managerName" type="java.lang.String"/>
        <property column="manager_email" length="250" name="managerEmail" type="java.lang.String"/>
    </class>
</hibernate-mapping>
